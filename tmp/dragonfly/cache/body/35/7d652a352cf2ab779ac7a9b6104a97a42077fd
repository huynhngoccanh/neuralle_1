(function() {
  $(document).ready(function() {
    $('.testimonials-carousel').owlCarousel({
      singleItem: true,
      loop: true,
      autoPlay: true,
      stopOnHover: true,
      pagination: true
    });
    $('.media-carousel').owlCarousel({
      singleItem: true,
      loop: true,
      autoPlay: true,
      stopOnHover: true,
      pagination: true
    });
    return $('.media-carousel .item').removeClass('hide');
  });

}).call(this);
